  SOURCE: s=userSource(id("graphdataset"))
  DATA: repeatedmeasure=col(source(s), name("repeatedmeasure"))
  DATA: cont_outcome=col(source(s), name("cont_outcome"))
  DATA: subject=col(source(s), name("subject"), unit.category())
  GUIDE: axis(dim(1), label("repeated measure"))
  GUIDE: axis(dim(2), label("outcome"))
  GUIDE: legend(aesthetic(aesthetic.color.interior), label("subject"))
  GUIDE: text.title(label("outcome"))
  ELEMENT: line(position( repeatedmeasure*cont_outcome), color.interior(subject), missing.wings(),transparency(transparency."0.5"))
