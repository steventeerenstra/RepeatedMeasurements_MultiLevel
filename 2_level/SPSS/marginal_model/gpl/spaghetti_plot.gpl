  SOURCE: s=userSource(id("graphdataset"))
  DATA: repeatedmeasure=col(source(s), name("repeatedmeasure_"))
  DATA: outcome=col(source(s), name("outcome_"))
  DATA: subject=col(source(s), name("subject_"), unit.category())
  GUIDE: axis(dim(1), label("repeated measure"))
  GUIDE: axis(dim(2), label("outcome"))
  GUIDE: legend(aesthetic(aesthetic.color.interior), label("subject"))
  GUIDE: text.title(label("spaghetti plot of outcome vs repeated measure for each subject"))
  ELEMENT: line(position( repeatedmeasure*outcome), color.interior(subject), missing.wings(),transparency(transparency."0.5"))